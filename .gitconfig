[alias]
	st = status
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	clear = reset --hard
[user]
	name = Sébastien Pécoul
	email = specoul@steamulo.com
[rebase]
  autosquash = true
[fetch]
  prune = true
[push]
	default = current
